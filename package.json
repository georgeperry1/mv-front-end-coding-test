{
  "name": "mv-front-end-coding-test",
  "version": "1.0.0",
  "description": "**Please upload this project to your preferred source control platform and share with Model Village. Place your work inside the `/test` folder**",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "yarn build",
    "build": "mkdir dist &> /dev/null || : && cp src/index.html dist/index.html && webpack-dev-server --mode development --content-base src/ --inline --hot",
    "build:prod": "webpack -p --mode production && cp src/index.html dist/index.html"
  },
